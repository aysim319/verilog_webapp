v0x6000008ec1b0_0 .var "q", 0 0;
v0x6000008ec240_0 .net "rstn", 0 0, v0x6000008ec5a0_0;  1 drivers
v0x6000008ec2d0_0 .net "t", 0 0, v0x6000008ec630_0;  1 drivers
E_0x600002fe1e40 .event posedge, v0x6000008ec120_0;
    .scope S_0x124807170;
T_0 ;
    %wait E_0x600002fe1e40;
    %load/vec4 v0x6000008ec240_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x6000008ec1b0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x6000008ec2d0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x6000008ec1b0_0;
    %nor/r;
    %assign/vec4 v0x6000008ec1b0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x6000008ec1b0_0;
    %assign/vec4 v0x6000008ec1b0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x124804eb0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x6000008ec360_0;
    %inv;
    %store/vec4 v0x6000008ec360_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x124804eb0;
T_2 ;
    %delay 20, 0;
    %load/vec4 v0x6000008ec480_0;
    %inv;
    %store/vec4 v0x6000008ec480_0, 0, 1;
    %jmp T_2;
