    .scope S_0x124804eb0;
T_3 ;
    %vpi_func 3 18 "$fopen" 32, "output_tff_tb.txt" {0 0 0};
    %store/vec4 v0x6000008ec3f0_0, 0, 32;
    %vpi_call/w 3 19 "$fwrite", v0x6000008ec3f0_0, "time,rstn,t,q\012" {0 0 0};
T_3.0 ;
    %wait E_0x600002fe1e40;
    %vpi_call/w 3 22 "$fwrite", v0x6000008ec3f0_0, "%g,%b,%b,%b\012", $time, v0x6000008ec5a0_0, v0x6000008ec630_0, v0x6000008ec510_0 {0 0 0};
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_0x1248041f0;
T_4 ;
    %vpi_func 3 34 "$random" 32 {0 0 0};
    %pad/s 5;
    %store/vec4 v0x6000008ec000_0, 0, 5;
    %end;
    .thread T_4, $init;
    .scope S_0x124804eb0;
T_5 ;
    %pushi/vec4 0, 0, 4;
    %split/vec4 1;
    %assign/vec4 v0x6000008ec480_0, 0;
    %split/vec4 1;
    %assign/vec4 v0x6000008ec630_0, 0;
    %split/vec4 1;
    %assign/vec4 v0x6000008ec360_0, 0;
    %assign/vec4 v0x6000008ec5a0_0, 0;
    %vpi_call/w 3 29 "$monitor", "T=%0t rstn=%0b t=%0d q=%0d", $time, v0x6000008ec5a0_0, v0x6000008ec630_0, v0x6000008ec510_0 {0 0 0};
    %pushi/vec4 2, 0, 32;
T_5.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_5.1, 5;
    %jmp/1 T_5.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x600002fe1e40;
    %jmp T_5.0;
T_5.1 ;
    %pop/vec4 1;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x6000008ec5a0_0, 0;
    %fork t_1, S_0x124804080;
    %jmp t_0;
    .scope S_0x124804080;
t_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6000008ec090_0, 0, 32;
T_5.2 ;
    %load/vec4 v0x6000008ec090_0;
    %cmpi/s 20, 0, 32;
    %jmp/0xz T_5.3, 5;
    %fork t_3, S_0x1248041f0;
    %jmp t_2;
    .scope S_0x1248041f0;
t_3 ;
    %load/vec4 v0x6000008ec000_0;
    %pad/u 64;
    %muli 1, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %vpi_func 3 35 "$random" 32 {0 0 0};
    %pad/s 1;
    %assign/vec4 v0x6000008ec630_0, 0;
    %end;
